#define _CRT_SECURE_NO_DEPRECATE
#include <stdio.h>
#include <locale.h>
#include <math.h>


//Для 221 группы
//Вариант 12
//Контрольный пример 1: При вводе точки с координатами (1;2) на консоль выводится: "Точка с координатами (1,000000;2,000000) ПРИНАДЛЕЖИТ данной закрашенной области";
//Контрольный пример 2: При вводе точки с координатами (2;5,1) на консоль выводится: "Точка с координатами (2,000000;5,100000) НЕ ПРИНАДЛЕЖИТ данной закрашенной области";

void main() {
    setlocale(LC_ALL, "RUS");

    double x, y;

    printf("Введите первую координату точки, x:");
    scanf("%lf", &x);

    printf("Введите вторую координату точки, y:");
    scanf("%lf", &y);

    printf("\nВведена точка с координатами (%lf;%lf)\n\n", x, y);

    if ((x >= -2 && y >= 2) && (x <= 3 && y <= 5) || (x >= 3 && y >= 2) && (x <= 5 && y <= 3))
        printf("Точка с координатами (%lf;%lf) ПРИНАДЛЕЖИТ данной закрашенной области\n\n", x, y);
    else
        printf("Точка с координатами (%lf;%lf) НЕ ПРИНАДЛЕЖИТ данной закрашенной области\n\n", x, y);
}
