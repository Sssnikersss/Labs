#define _CRT_SECURE_NO_DEPRECATE
#include <stdio.h>
#include <locale.h>


void main()
{
	setlocale(LC_ALL, "RUS");
  
  int years, ost4, ost100, ost400;
  
  printf("Введите год:");
  scanf("%d", &years);
  printf("Введён %d год", years);
  
  ost4 = years % 4;
  ost100 = years % 100;
  ost400 = years % 400;
  
  if((ost4 = 0 && ost100 != 0) || ost400 = 0) 
   {
      printf("Год %d високосный", years);
   }
  else
   {
      printf("Год %d не високосный", years);
   }
}
