double* full_elements(double* ptr_array, int n) {
    double a;
    int i = 0;
    for (a = q; i < n; a += ((q1 - q) / n), i++) {
        ptr_array[i] = func(a);
    }
}

int put_elements(double* ptr_array, int n) {
    for (int i = 0; i < n; i++) {
        printf("A[%d] = %lf\n", i, ptr_array[i]);
    }
}

double func(double x) {
    double result;
    result = 5 * pow(x, 3) + 2 * pow(x, 2) - 15 * x - 6;
    return result;
}
