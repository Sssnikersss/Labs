#define _CRT_SECURE_NO_DEPRECATE
#include <stdio.h>
#include <locale.h>
#include <math.h>

int main() {
	setlocale(LC_ALL, "RUS");
	float N;
	double x = 1.3, result;

	printf("Функция: y=5x^3+2x^2-15x-6\n");
	printf("Интервал: [1.3;3.4]\n\n");
	printf("Введите шаг табуляции:");
	scanf("%f", &N);
	printf("---------------\n");
	printf("|   x  | f(x) |\n");
	printf("|------|------|\n");
	for (; x <= 3.4; x += N) {
		result = 5 * pow(x, 3) + 2 * pow(x, 2) - 15 * x - 6;
		printf("|%6.2lf|%6.2lf|\n", x, result);
	}
	return 0;
}
