#define _CRT_SECURE_NO_DEPRECATE
#include <stdio.h>
#include <locale.h>


int main() {
	setlocale(LC_ALL, "RUS");
	int n, m, s = 0, num = 0;

	printf("Введите значение n: ");
	scanf("%d", &n);
	printf("Введите значение m: ");
	scanf("%d", &m);
	
	for (; m >= n; m--, num++) {
		s += m;
		printf("Выполнено %d раз\n", num + 1);
	}
	printf("Результат: %d\n", s);
	return 0;
}
